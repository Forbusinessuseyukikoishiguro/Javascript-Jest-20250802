## ğŸ“ ãƒ¡ã‚¤ãƒ³å®Ÿè£…ãƒ•ã‚¡ã‚¤ãƒ«

### src/calculator.js
```javascript
/**
 * é›»å“æ©Ÿèƒ½ã®å®Ÿè£…
 * è¶³ã—ç®—ã€å¼•ãç®—ã€ãã®ä»–ã®åŸºæœ¬æ¼”ç®—ã‚’æä¾›
 */

/**
 * è¶³ã—ç®—ã‚’è¡Œã†é–¢æ•°
 * @param {number} a - æœ€åˆã®æ•°å€¤
 * @param {number} b - 2ç•ªç›®ã®æ•°å€¤
 * @returns {number} a ã¨ b ã®åˆè¨ˆ
 * @throws {Error} å¼•æ•°ãŒæ•°å€¤ã§ãªã„å ´åˆ
 */
function add(a, b) {
  // å‹ãƒã‚§ãƒƒã‚¯ï¼šæ•°å€¤ä»¥å¤–ã®å ´åˆã¯ã‚¨ãƒ©ãƒ¼
  if (typeof a !== 'number' || typeof b !== 'number') {
    throw new Error('å¼•æ•°ã¯æ•°å€¤ã§ã‚ã‚‹å¿…è¦ãŒã‚ã‚Šã¾ã™');
  }
  
  // NaNï¼ˆNot a Numberï¼‰ã®ãƒã‚§ãƒƒã‚¯
  if (isNaN(a) || isNaN(b)) {
    throw new Error('å¼•æ•°ã¯NaNã§ã‚ã£ã¦ã¯ã„ã‘ã¾ã›ã‚“');
  }
  
  return a + b;
}

/**
 * å¼•ãç®—ã‚’è¡Œã†é–¢æ•°
 * @param {number} a - å¼•ã‹ã‚Œã‚‹æ•°
 * @param {number} b - å¼•ãæ•°
 * @returns {number} a ã‹ã‚‰ b ã‚’å¼•ã„ãŸçµæœ
 * @throws {Error} å¼•æ•°ãŒæ•°å€¤ã§ãªã„å ´åˆ
 */
function subtract(a, b) {
  // å‹ãƒã‚§ãƒƒã‚¯ï¼šæ•°å€¤ä»¥å¤–ã®å ´åˆã¯ã‚¨ãƒ©ãƒ¼
  if (typeof a !== 'number' || typeof b !== 'number') {
    throw new Error('å¼•æ•°ã¯æ•°å€¤ã§ã‚ã‚‹å¿…è¦ãŒã‚ã‚Šã¾ã™');
  }
  
  // NaNï¼ˆNot a Numberï¼‰ã®ãƒã‚§ãƒƒã‚¯
  if (isNaN(a) || isNaN(b)) {
    throw new Error('å¼•æ•°ã¯NaNã§ã‚ã£ã¦ã¯ã„ã‘ã¾ã›ã‚“');
  }
  
  return a - b;
}

/**
 * è¤‡æ•°ã®æ•°å€¤ã‚’è¶³ã—ç®—ã™ã‚‹é–¢æ•°
 * @param {...number} numbers - è¶³ã—ç®—ã™ã‚‹æ•°å€¤ãŸã¡
 * @returns {number} å…¨ã¦ã®æ•°å€¤ã®åˆè¨ˆ
 * @throws {Error} å¼•æ•°ãŒæ•°å€¤ã§ãªã„å ´åˆ
 */
function addMany(...numbers) {
  // å¼•æ•°ãŒãªã„å ´åˆã¯0ã‚’è¿”ã™
  if (numbers.length === 0) {
    return 0;
  }
  
  // å…¨ã¦ã®å¼•æ•°ãŒæ•°å€¤ã‹ãƒã‚§ãƒƒã‚¯
  for (let i = 0; i < numbers.length; i++) {
    if (typeof numbers[i] !== 'number' || isNaN(numbers[i])) {
      throw new Error(`å¼•æ•°${i + 1}ãŒç„¡åŠ¹ãªæ•°å€¤ã§ã™: ${numbers[i]}`);
    }
  }
  
  // reduceé–¢æ•°ã‚’ä½¿ã£ã¦åˆè¨ˆã‚’è¨ˆç®—
  return numbers.reduce((sum, current) => sum + current, 0);
}

/**
 * çµ¶å¯¾å€¤ã®å·®ã‚’è¨ˆç®—ã™ã‚‹é–¢æ•°
 * @param {number} a - 1ã¤ç›®ã®æ•°å€¤
 * @param {number} b - 2ã¤ç›®ã®æ•°å€¤
 * @returns {number} |a - b| ã®çµæœ
 */
function absoluteDifference(a, b) {
  if (typeof a !== 'number' || typeof b !== 'number') {
    throw new Error('å¼•æ•°ã¯æ•°å€¤ã§ã‚ã‚‹å¿…è¦ãŒã‚ã‚Šã¾ã™');
  }
  
  if (isNaN(a) || isNaN(b)) {
    throw new Error('å¼•æ•°ã¯NaNã§ã‚ã£ã¦ã¯ã„ã‘ã¾ã›ã‚“');
  }
  
  return Math.abs(a - b);
}

// é–¢æ•°ã‚’ã‚¨ã‚¯ã‚¹ãƒãƒ¼ãƒˆï¼ˆä»–ã®ãƒ•ã‚¡ã‚¤ãƒ«ã‹ã‚‰ä½¿ãˆã‚‹ã‚ˆã†ã«ã™ã‚‹ï¼‰
module.exports = {
  add,
  subtract,
  addMany,
  absoluteDifference
};
```

