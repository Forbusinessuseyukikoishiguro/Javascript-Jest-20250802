  const isCI = process.env.CI === 'true';
  
  (!isCI ? test : test.skip)('ãƒ­ãƒ¼ã‚«ãƒ«ç’°å¢ƒã§ã®ã¿å®Ÿè¡Œ', () => {
    // CIç’°å¢ƒã§ã¯ã‚¹ã‚­ãƒƒãƒ—ã€ãƒ­ãƒ¼ã‚«ãƒ«ã§ã¯å®Ÿè¡Œ
  });
});

// ======================
// 10. package.json ã§ã®ãƒ†ã‚¹ãƒˆå®Ÿè¡Œã‚ªãƒ—ã‚·ãƒ§ãƒ³
// ======================

