// ãƒ†ã‚¹ãƒˆå¯¾è±¡ã®é–¢æ•°ï¼ˆä¾‹ï¼‰
const calculator = {
  add: (a, b) => a + b,
  subtract: (a, b) => a - b,
  multiply: (a, b) => a * b,
  divide: (a, b) => {
    if (b === 0) throw new Error('Division by zero');
    return a / b;
  },
  // ã¾ã å®Ÿè£…ã—ã¦ã„ãªã„æ©Ÿèƒ½
  power: (base, exponent) => {
    throw new Error('Not implemented yet');
  }
};

describe('Calculator ã‚¯ãƒ©ã‚¹', () => {
  
  // âœ… å®Ÿè£…æ¸ˆã¿æ©Ÿèƒ½ã®ãƒ†ã‚¹ãƒˆ
  test('è¶³ã—ç®—', () => {
    expect(calculator.add(2, 3)).toBe(5);
  });
  
  test('å¼•ãç®—', () => {
    expect(calculator.subtract(10, 4)).toBe(6);
  });
  
  test('æ›ã‘ç®—', () => {
    expect(calculator.multiply(3, 4)).toBe(12);
  });
  
  test('å‰²ã‚Šç®—', () => {
    expect(calculator.divide(10, 2)).toBe(5);
  });
  
  test('ã‚¼ãƒ­é™¤ç®—ã‚¨ãƒ©ãƒ¼', () => {
    expect(() => calculator.divide(10, 0)).toThrow('Division by zero');
  });
  
  // â­ï¸ æœªå®Ÿè£…æ©Ÿèƒ½ã¯ä¸€æ™‚çš„ã«ã‚¹ã‚­ãƒƒãƒ—
  describe.skip('ç´¯ä¹—æ©Ÿèƒ½ï¼ˆæœªå®Ÿè£…ï¼‰', () => {
    test('2ã®3ä¹—ã¯8', () => {
      expect(calculator.power(2, 3)).toBe(8);
    });
    
    test('è² ã®æŒ‡æ•°', () => {
      expect(calculator.power(2, -1)).toBe(0.5);
    });
  });
});

